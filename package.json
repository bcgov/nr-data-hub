  "dependencies": {
    "@adobe/jsonschema2md": "^7.1.5",
    "json-schema-to-typescript": "^13.1.2"
  },
  "scripts": {
    "autogenerate": "yarn run generate-typescript-schemas && yarn run generate-docs",
    "generate-typescript-schemas": "npx ts-node src/autogenerate/interfaces.ts",
    "generate-docs": "jsonschema2md -o docs/schemas -d schemas -h false -x - -n -s definitions"
  }
}
